{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/out-tsc",
        "baseUrl": ".",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ],
        "paths": {
            "@angular/*": ["node_modules/@angular/*"],
            "@angular-devkit/*": ["node_modules/@angular-devkit/*"],
            "rxjs/*": ["node_modules/rxjs/*"],
            "@ngx-translate/*": ["node_modules/@ngx-translate/*"],
            "@nguniversal/*": ["node_modules/@nguniversal/*"],
            "@ngtools/*": ["node_modules/@ngtools/*"],
            "ng-socket-io": ["node_modules/ng-socket-io"],
            "ngx-bootstrap": ["node_modules/ngx-bootstrap"],
            "ngx-bootstrap/*": ["node_modules/ngx-bootstrap/*"],
            "ngx-toastr": ["node_modules/ngx-toastr"],
            "ngx-mask": ["node_modules/ngx-mask"],
            "@stemy/*": ["node_modules/@stemy/*"]
        }
    }
}
